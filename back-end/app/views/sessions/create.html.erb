<h1>Sessions#create</h1>
<p>Find me in app/views/sessions/create.html.erb</p>

<h1>Sign Up</h1>
<%= form_for @user do |f|%>
  <%= f.text_field :email%><br>
  <%= f.text_field :first_name%><br>
  <%= f.text_field :last_name%><br>
  <%= f.text_field :username%><br>
  <%= f.label :password%><br>
  <%= f.password_field :password%><br>
  <%= f.submit %>
<%end%>